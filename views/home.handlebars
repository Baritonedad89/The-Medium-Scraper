<div class='container article-container'>
  <div class="row" id="articles-row">
    <div class="col l12">



      {{#each articles}}
      {{#unless this.isSaved}}
      <div class="row" id="row-{{this._id}}">
        <div class="col l12">
          <div class="card light-blue darken-4">
            <div class="card-content white-text">
              <span class="card-title"><a href="{{this.url}}" target="_blank" data-id="{{this._id}}">{{this.headline}}</a></span>
              <p data-id="{{this._id}}">{{this.summary}}</p>
            </div>
            <div class="card-action">
              <a class="waves-effect waves-light btn" id="save-btn" isSaved={{this.isSaved}} data-id={{this._id}}>Save Article</a>

            </div>
          </div>
        </div>
      </div>
      {{/unless}}
      {{/each}}
      